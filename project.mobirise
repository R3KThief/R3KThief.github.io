" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n\n            <div class=\"media-content align-right\">\n                <h1 class=\"mbr-section-title mbr-white pb-3 mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><b>\n                    Landing Pad</b></h1>\n                <div class=\"mbr-section-text mbr-white pb-3\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">\n                        Watch the video located to the right of this text block. &nbsp;It will give you an idea of how Jasonic<br>Warhammer works (I do not own Warhammer)</p>\n                </div>\n                <div class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-md btn-primary\" href=\"https://www.games-workshop.com/en-US/Warhammer-40-000\" data-app-placeholder=\"Type Text\">What is WH40K?</a> <a class=\"btn btn-md btn-primary\" href=\"https://www.games-workshop.com/en-US/Warhammer-40-000\" data-app-placeholder=\"Type Text\">What is WH40K?</a>\n                        <a class=\"btn btn-md btn-primary\" href=\"MainControlPanel.html\" data-app-placeholder=\"Type Text\">Continue to Control Panel<br></a></div>\n            </div>\n\n            <div mbr-video class=\"mbr-figure\" mbr-style=\"{'width': mediaSize + '%'}\">\n                <iframe src=\"https://www.youtube.com/watch?v=2hASvJz9VkA&t=764s&loop=0&autoplay=0\"></iframe>\n            </div>\n\n        </div>\n  